{
    "name": "maarsson/laravel-nuxtjs-router",
    "description": "Laravel route service provider for NuxtJS frontend",
    "type": "package",
    "keywords": ["nuxtjs", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "VMaarsson",
            "email": "viktor@maarsson.se"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Maarsson\\NuxtJsRouter\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Maarsson\\NuxtJsRouter\\NuxtJsRouterServiceProvider"
            ]
        }
    }
}
